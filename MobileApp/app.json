{
  "expo": {
    "name": "Co-Op",
    "slug": "co-op",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "coop",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f1012"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coop.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["https", "http"]
      },
      "associatedDomains": [
        "applinks:co-op-dev.vercel.app",
        "applinks:co-op.vercel.app"
      ]
    },
    "android": {
      "package": "com.coop.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0f1012"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "co-op-dev.vercel.app",
              "pathPrefix": "/auth/callback"
            },
            {
              "scheme": "https",
              "host": "co-op.vercel.app",
              "pathPrefix": "/auth/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "androidStatusBar": {
      "translucent": true,
      "backgroundColor": "transparent"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
